# AutoYield Backend Environment Variables
# Copy this file to .env and fill in the values

# =============================================================================
# Automation Session Key
# Generated by: npm run generate-session-key
# =============================================================================

# Private key for signing automation UserOps (KEEP SECRET)
AUTOMATION_PRIVATE_KEY=0x_run_npm_run_generate-session-key_to_get_this

# Public address of the automation key (share with Jackson for factory)
AUTOMATION_PUBLIC_ADDRESS=0x_run_npm_run_generate-session-key_to_get_this

# =============================================================================
# CDP Bundler / Paymaster (Task 2)
# Get from: https://portal.cdp.coinbase.com
# =============================================================================

# CDP_BUNDLER_URL=https://api.developer.coinbase.com/rpc/v1/base/YOUR_API_KEY

# =============================================================================
# Contract Addresses (Jackson provides after deployment)
# =============================================================================

# AUTO_YIELD_MODULE_ADDRESS=0x...
# FACTORY_ADDRESS=0x...

# =============================================================================
# Server Config
# =============================================================================

PORT=3001
